{
    "coc.preferences.formatOnSaveFiletypes": [
        "c++",
        "json",
        "go",
        "c",
        "python"
    ],
    "json.schemaDownload.enable": true,
    "go.goplsEnv": {
        "GOPROXY": "https://goproxy.cn,direct"
    },
    "latex.build.args": [
        "-pdfxe",
        "-interaction=nonstopmode",
        "-synctex=1"
    ],
    "latex.build.executable": "latexmk",
    "latex.build.outputDirectory": "outputs"
}
